{
  "name": "workdaytracker",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "tunnel": "expo start --tunnel",
    "dev": "expo start --clear",
    "dev:perf": "ENABLE_PERFORMANCE=true expo start --clear",
    "dev:network": "ENABLE_NETWORK_LOGGING=true expo start --clear",
    "tsc": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-firebase/app": "^21.5.0",
    "@react-native-firebase/auth": "^21.5.0",
    "@react-native-firebase/firestore": "^21.5.0",
    "@react-navigation/native": "^7.0.3",
    "@react-navigation/stack": "^7.0.5",
    "@tanstack/react-query": "^5.60.6",
    "date-fns": "^4.1.0",
    "expo": "~52.0.8",
    "expo-status-bar": "~2.0.0",
    "firebase": "^11.0.2",
    "react": "18.3.1",
    "react-native": "0.76.2",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-svg": "15.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-firebase/perf": "^21.5.0",
    "@react-native/eslint-config": "^0.76.2",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ft-flow": "^2.0.3",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "prettier": "^3.3.3",
    "react-error-boundary": "^4.1.2",
    "reactotron-react-native": "^5.1.10",
    "typescript": "^5.3.3"
  },
